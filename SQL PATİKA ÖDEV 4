1)
SELECT DISTINCT replacement_cost
FROM film ;

2)
SELECT count ( DISTINCT replacement_cost)
FROM film ;

3)
SELECT count ( title)
FROM film 
WHERE title LIKE 'T%' AND rating = 'G';

4)
SELECT COUNT(country)
FROM country 
WHERE LENGTH(country) = 5 ;

5)
SELECT COUNT(city)
FROM city
WHERE city ILIKE '%R';
